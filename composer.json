{
    "name": "keenthemes/metronic-laravel",
    "type": "project",
    "description": "The Metronic in the Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "anlutro/l4-settings": "^1.4.1",
        "guzzlehttp/guzzle": "^7.9.3",
        "laravel/framework": "^11.0",
        "laravel/socialite": "^5.20",
        "laravel/tinker": "^2.10.1",
        "laraveles/spanish": "^1.5",
        "maatwebsite/excel": "^3.1.64",
        "spatie/laravel-activitylog": "^4.10.1",
        "spatie/laravel-permission": "^6.1.0",
        "yajra/laravel-datatables-buttons": "^11.2.2",
        "yajra/laravel-datatables-oracle": "~11.1.6"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.1.0",
        "fakerphp/faker": "^1.24.0",
        "laravel/breeze": "^2.0",
        "laravel/sail": "^1.42.0",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.5.19",
        "spatie/laravel-ignition": "^2.9.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    },
    "repositories": [
    ]
}
